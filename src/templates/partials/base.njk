<!DOCTYPE html>
<html lang="es-MX">
  <head>

    {% include 'partials/metatags.njk' %}

    {% block meta %}{% endblock %}

    {% block extra_css %}{% endblock %}
    <link rel="stylesheet" href="css/{{ projectFilesName }}.libs.css">
    <link rel="stylesheet" href="css/main.css">
    <link runat="server" rel="shortcut icon" href="img/favicon/wicore-favicon.png" type="image/x-icon"/>
    <link runat="server" rel="icon" href="img/favicon/wicore-favicon.png" type="image/ico"/>
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <link rel="canonical" href="http://wicore.mx/" />
    <title>{% block title %}{% endblock %} | Wicore</title>

    {% include 'partials/googleTagManagerScript.njk' %}

    {% include 'partials/hotjarTracking.njk' %}

  </head>
  <body class="{% if section == 'ful' %}ful--view{% endif %}">

    <div id="page">
      {% include 'partials/navbar.njk' %}
      <main>
        {% block content %}{% endblock %}
      </main>
      {% include 'partials/footer.njk' %}
    </div>
    {% block js_libs_config %}
    {% endblock %}
    <script src="js/{{ projectFilesName }}.libs.js"></script>
    {% block js_project_config %}
    {% endblock %}
    <script src="js/{{ projectFilesName }}.js"></script>
    {% block js_inline %}
    {% endblock %}
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
      AOS.init();
    </script>
    <!-- Start Zoho Code -->
    <script type="text/javascript">
      var $zoho=$zoho || {};$zoho.salesiq = $zoho.salesiq ||
              {widgetcode:"b62f39c6c8cce823338b40b8ec93d30f7fd7f6e8785cc96401ab9ec562b9e291", values:{},ready:function(){}};
      var d=document;s=d.createElement("script");s.type="text/javascript";s.id="zsiqscript";s.defer=true;
      s.src="https://salesiq.zoho.com/widget";t=d.getElementsByTagName("script")[0];t.parentNode.insertBefore(s,t);d.write("<div id='zsiqwidget'></div>");
